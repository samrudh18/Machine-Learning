   "source": [
    "import time\n",
    "\n",
    "np.random.seed(0)\n",
    "X=2.5*np.random.randn(10000)+1.5      #X values: mean=1.5,sd=2.5,num=10000\n",
    "res=1.5*np.random.randn(10000)        #residual values: mean=0, sd=1.5\n",
    "y=2+0.3*X+res                         #y=c+mx+res\n",
    "\n",
    "start=0\n",
    "stop=0\n",
    "\n",
    "true_a=0.3\n",
    "true_b=2\n",
    "error=10/100 #10% error\n",
    "\n",
    "sizes=np.linspace(start=1,stop=X.shape[0],num=X.shape[0]).astype(int)\n",
    "max_iters=1000\n",
    "for bsize in sizes:\n",
    "    tik=time.time()\n",
    "    X,y=shuffle(X,y)\n",
    "    a,b,costs=linreg_gradient_descent(X,y,batch_size=bsize,init_a=0,init_b=0,learn_rate=0.1,n_iter=max_iters,tol=1e-06,max_iters=max_iters)\n",
    "    tok=time.time()\n",
    "    if (true_a-(0.1*true_a))<a< (true_a+(error*true_a)) and (true_b-(error*true_b))<b< (true_b+(error*true_b)): #10% error\n",
    "        break\n",
    "\n",
    "print(f'The optimal batchsize is {bsize} with +/-{int(error*100)}% error. The slope={a} (true=0.3), intercept={b} (true=2) and runs in {tok-tik} ms')"
