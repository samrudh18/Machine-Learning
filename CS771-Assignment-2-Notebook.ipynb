  {
   "cell_type": "code",
   "execution_count": 69,
   "metadata": {},
   "outputs": [],
   "source": [
    "def gradient_descent(gradient,init_,learn_rate,n_iter=50,tol=1e-06):\n",
    "    x=init_\n",
    "    for _ in range(n_iter):\n",
    "        delta=learn_rate*-gradient(x)    #rate and direction of change in f(x) with respect to current value of x (dy/dx) \n",
    "        if np.all(np.abs(delta))<=tol:\n",
    "            break\n",
    "        x+=delta                         #moving in the direction opposite to the gradient, towards the minima \n",
    "    return np.around(x,decimals=3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 70,
   "metadata": {},
   "outputs": [],
   "source": [
    "def shuffle(array1,array2):\n",
    "\n",
    "    shuffler = np.random.permutation(len(array1)) #get random permutation of numbers of specified n\n",
    "    array1_shuffled = array1[shuffler]\n",
    "    array2_shuffled = array2[shuffler]\n",
    "    \n",
    "    return array1_shuffled,array2_shuffled  "
   ]
  },
