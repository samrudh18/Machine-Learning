   "metadata": {},
   "source": [
    "#### Mini-batch SGD"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 82,
   "metadata": {},
   "outputs": [],
   "source": [
    "def linreg_gradient_descent(X,y,batch_size,init_a=0,init_b=0,learn_rate=0.1,n_iter=100,tol=1e-06,max_iters=None):\n",
    "    costs=[]\n",
    "    a=init_a\n",
    "    b=init_b\n",
    "    n=X.shape[0]\n",
    "    \n",
    "    batches=np.arange(0,n,step=batch_size)                   #create batches according to specified batch size\n",
    "    batches=np.insert(batches,len(batches),n)\n",
    "\n",
    "    grad_a=lambda a,b,X,y,yhat: (-2/n)* np.sum((y-yhat) * X)   #partial derivative with respect to a (L2 norm) \n",
    "    grad_b=lambda a,b,X,y,yhat: (-2/n)* np.sum(y-yhat)         #partial derivative with respect to b (L2 norm)\n",
    "    \n",
    "    iters=0\n",
    "    for _ in range(n_iter):\n",
    "        X,y=shuffle(X,y)\n",
    "        for i in range(len(batches)-1):\n",
    "            X_batch=X[batches[i]:batches[i+1]]                #select batches instead of whole array \n",
    "            y_batch=y[batches[i]:batches[i+1]]\n",
    "            \n",
    "            yhat=a*X_batch+b  #predicted y value\n",
    "            delta_a=learn_rate*-grad_a(a,b,X_batch,y_batch,yhat)       #change with respect to a\n",
    "            delta_b=learn_rate*-grad_b(a,b,X_batch,y_batch,yhat)       #change with respect to b\n",
    "            if np.all(np.abs(delta_a))<=tol and np.all(np.abs(delta_b))<=tol:\n",
    "                break\n",
    "            a+=delta_a\n",
    "            b+=delta_b\n",
    "\n",
    "            MSE=np.mean((y_batch-yhat)**2)                 #calculate MSE using current prediction\n",
    "            costs.append(MSE)               \n",
    "            \n",
    "            iters+=1\n",
    "            if max_iters and iters>=max_iters:\n",
    "                return round(a,3), round(b,3),costs\n",
    "                \n",
    "    return round(a,3), round(b,3),costs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "metadata": {},
   "outputs": [],
   "source": [
    "import time\n",
    "\n",
    "np.random.seed(0)\n",
    "X=2.5*np.random.randn(10000)+1.5      #X values: mean=1.5,sd=2.5,num=10000\n",
    "res=1.5*np.random.randn(10000)        #residual values: mean=0, sd=1.5\n",
    "y=2+0.3*X+res                         #y=c+mx+res\n",
    "\n",
    "sizes=[1,10,100,1000,5000,10000]\n",
    "sizes=np.geomspace(start=1, stop=X.shape[0], num=6).astype(int)       #batch sizes\n",
    "total_results=[]\n",
    "max_iters=1000             \n",
    "for bsize in sizes:\n",
    "    tik=time.time()         #starting time\n",
    "    a,b,costs=linreg_gradient_descent(X,y,batch_size=bsize,init_a=0,init_b=0,learn_rate=0.1,n_iter=max_iters,tol=1e-06,max_iters=max_iters)\n",
    "    tok=time.time()         #stopping time\n",
    "    total_results.append((costs,a,b,costs[-1],tok-tik))"
   ]
  },
